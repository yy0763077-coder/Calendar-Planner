page {
  height: 100%;
  background: #f5f5f5;
  font-family: -apple-system, BlinkMacSystemFont, 'PingFang SC', 'Helvetica Neue', 'Microsoft YaHei', sans-serif;
  -webkit-font-smoothing: antialiased;
}

button {
  padding: 0;
  margin: 0;
  line-height: inherit;
  background: transparent;
}

button::after {
  border: none;
}
